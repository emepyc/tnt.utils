{
  "name": "tnt.utils",
  "version": "0.0.1",
  "description": "Utils for tnt",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter > coverage/lcov.info && gulp test && gulp coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emepyc/tnt.utils"
  },
  "keywords": [
    "tnt",
    "biojs",
    "javascript",
    "utils"
  ],
  "author": "Miguel Pignatelli",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/emepyc/tnt.utils/issues"
  },
  "homepage": "https://github.com/emepyc/tnt.utils",
  "devDependencies": {
    "blanket": "^1.1.6",
    "browserify": "~4.2.0",
    "chai": "^1.9.1",
    "coveralls": "^2.11.1",
    "gulp": "^3.8.8",
    "gulp-browserify": "^0.5.0",
    "gulp-coveralls": "^0.1.3",
    "gulp-gzip": "0.0.8",
    "gulp-jshint": "^1.8.4",
    "gulp-mocha": "^1.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-uglify": "^1.0.1",
    "gulp-watch": "^1.0.2",
    "mkdirp": "^0.5.0",
    "mocha": "~1.20.1",
    "mocha-lcov-reporter": "0.0.1",
    "uglify-js": "~2.4.15",
    "watchify": "^1.0.2"
  },
  "config" : {
    "blanket" : {
      "pattern" : ["src"],
